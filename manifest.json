{
  "manifest_version": 3,
  "name": "TiTra",
  "version": "1.0",
  "description": "Displays Punch-in and Break Time from the attendance page.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon48.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://timetracker.newagesmb.com/*"
  ]
}
