{
    "manifest_version": 3,
    "name": "Time Tracker Popup",
    "version": "1.0",
    "description": "Displays Punch-in and Break Time from the attendance page.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "https://timetracker.newagesmb.com/*"
    ]
  }
  